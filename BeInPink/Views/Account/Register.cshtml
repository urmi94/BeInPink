@model BeInPink.Models.RegisterViewModel
@{
    ViewBag.Title = "Register";
}

<h2>@ViewBag.Title.</h2>
<div class="form-group">
    <h4>Create a new account.</h4>
    <div class="col-md-3">
        @using (Html.BeginForm("RegisterCoach", "Account", FormMethod.Post, new { @class = "", role = "form" }))
        {
            @Html.AntiForgeryToken()
            <input type="submit" class="btn btn-default" value="I want to give fitness services" />
           
        }
    </div>
    <div class="">
        @using (Html.BeginForm("RegisterClient", "Account", FormMethod.Post, new { @class = "", role = "form" }))
        {
            @Html.AntiForgeryToken()
            <input type="submit" class="btn btn-default" value="I am seeking fitness" />
           
        }
        </div>

    </div>
    @section Scripts {
        @Scripts.Render("~/bundles/jqueryval")
    }
